{
  "name": "settlement",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod --aot",
    "start": "ng serve --host 0.0.0.0 --disableHostCheck --proxy-config proxy.conf.js",
    "lint": "ng lint && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "prettier": "prettier --write \"./{src}/**/*.{ts,js,html,scss}\"",
    "prettier:check": "prettier --list-different \"./{src}/**/*.{ts,js,html,scss}\"",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean --sort --marker extract",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^7.2.3",
    "@angular/cdk": "^7.0.1",
    "@angular/common": "^7.2.3",
    "@angular/compiler": "^7.2.3",
    "@angular/core": "^7.2.3",
    "@angular/flex-layout": "^7.0.0-beta.24",
    "@angular/forms": "^7.2.3",
    "@angular/http": "^7.2.3",
    "@angular/material": "^7.0.1",
    "@angular/platform-browser": "^7.2.3",
    "@angular/platform-browser-dynamic": "^7.2.3",
    "@angular/router": "^7.2.3",
    "@ngx-translate/core": "^11.0.1",
    "angular-calendar": "^0.27.8",
    "core-js": "^2.6.3",
    "date-fns": "^1.30.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "material-design-icons-iconfont": "^4.0.5",
    "moment": "^2.24.0",
    "ngx-bootstrap": "^4.1.1",
    "ngx-toastr": "^10.0.4",
    "rxjs": "^6.4.0",
    "zone.js": "^0.9.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "^7.2.3",
    "@angular/language-service": "^7.2.3",
    "@biesbjerg/ngx-translate-extract": "^2.3.4",
    "@ngx-rocket/scripts": "^3.0.0",
    "@types/jasmine": "^3.3.8",
    "@types/jasminewd2": "^2.0.3",
    "@types/jwt-decode": "^2.2.1",
    "@types/node": "^8.10.13",
    "codelyzer": "^5.0.0",
    "htmlhint": "^0.11.0",
    "https-proxy-agent": "^2.2.3",
    "husky": "^1.3.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "prettier": "^1.16.3",
    "pretty-quick": "^1.10.0",
    "protractor": "~5.4.0",
    "puppeteer": "^1.12.0",
    "stylelint": "~10.0.1",
    "stylelint-config-prettier": "^5.1.0",
    "stylelint-config-recommended-scss": "~3.3.0",
    "stylelint-config-standard": "~18.3.0",
    "stylelint-scss": "~3.6.0",
    "ts-node": "~8.1.0",
    "tslint": "~5.16.0",
    "tslint-config-prettier": "^1.14.0",
    "typescript": "~3.2.0"
  },
  "prettier": {
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.scss",
        "options": {
          "singleQuote": false
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
